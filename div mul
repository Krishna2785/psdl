#include <xc.h>
#include<stdio.h>
#include<stdlib.h>
#include<pic18f4550.h>

void main(void) {
    int num1 = 0x23, num2 = 0x10;
    int prod = 0;
    for(int i = 1; i <= num2; i++){
        prod = prod + num1;
    }
    TRISB = 0;
    PORTB = prod;
}


Program 2 
 #include <xc.h>
#include<stdio.h>
#include<stdlib.h>
#include<pic18f4550.h>

void main(void) {
    int MUL, DIV;
    MUL = 0;
    DIV = 0;
    MUL = 0x04 * 0x01;
    DIV = 0x06/0x02;
    
    TRISD = 0;
    PORTD = MUL;
    
    TRISC = 0;
    PORTC = DIV;
}
